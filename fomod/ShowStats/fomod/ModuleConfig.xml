<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="http://qconsulting.ca/fo3/ModConfig5.0.xsd">
  <moduleName position="Left" colour="000000">ShowStats</moduleName>
  <installSteps order="Explicit">
    <installStep name="main">
      <!--<designer.metadata.do.not.edit> {"name":"main"}-->
      <optionalFileGroups order="Explicit">
        <group name="main" type="SelectAll">
          <!--<designer.metadata.do.not.edit> {"name":"main"}-->
          <plugins order="Ascending">
            <plugin name="main">
              <!--<designer.metadata.do.not.edit> {"name":"main"}-->
              <description>main files</description>
              <image path="img\title.png"/>
              <files>
                <folder source="main" destination="." priority="0" alwaysInstall="true" installIfUsable="false"/>
              </files>
              <typeDescriptor>
                <type name="Required"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
    <installStep name="gui">
      <!--<designer.metadata.do.not.edit> {"name":"gui"}-->
      <optionalFileGroups order="Explicit">
        <group name="gui" type="SelectExactlyOne">
          <!--<designer.metadata.do.not.edit> {"name":"gui"}-->
          <plugins order="Ascending">
            <plugin name="standard">
              <!--<designer.metadata.do.not.edit> {"name":"standard"}-->
              <description>standard</description>
              <image path="img\standard.png"/>
              <files>
                <file source="gui\standard\ShowStats.swf" destination="Interface\" priority="0" alwaysInstall="false" installIfUsable="false"/>
              </files>
              <typeDescriptor>
                <type name="Recommended"/>
              </typeDescriptor>
            </plugin>
            <plugin name="paper">
              <!--<designer.metadata.do.not.edit> {"name":"paper"}-->
              <description>paper</description>
              <image path="img\paper.png"/>
              <files>
                <file source="gui\paper\ShowStats.swf" destination="Interface\" priority="0" alwaysInstall="false" installIfUsable="false"/>
              </files>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
            <plugin name="paper_light">
              <!--<designer.metadata.do.not.edit> {"name":"paper_light"}-->
              <description/>
              <image path="img\paper_light.png"/>
              <files>
                <file source="gui\paper_light\ShowStats.swf" destination="Interface\" priority="0" alwaysInstall="false" installIfUsable="false"/>
              </files>
              <typeDescriptor>
                <type name="Optional"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
    <installStep name="finish">
      <!--<designer.metadata.do.not.edit> {"name":"finish"}-->
      <optionalFileGroups order="Explicit">
        <group name="finish" type="SelectAll">
          <!--<designer.metadata.do.not.edit> {"name":"finish"}-->
          <plugins order="Ascending">
            <plugin name="finish">
              <!--<designer.metadata.do.not.edit> {"name":"finish"}-->
              <description>finish</description>
              <typeDescriptor>
                <type name="Required"/>
              </typeDescriptor>
            </plugin>
          </plugins>
        </group>
      </optionalFileGroups>
    </installStep>
  </installSteps>
</config>
